{
  "schemaVersion": 1,
  "id": "infernitemod",
  "version": "${version}",
  "name": "Infernite",
  "description": "This mod adds an upgrade to Netherite to infuse it with more of the power of the Nether",
  "authors": [
    "BrodieBroadcasts"
  ],
  "contact": {
    "repo": "https://github.com/flippycereal/Infernite"
  },
  "license": "unlicense",
  "icon": "assets/infernitemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "net.flippycereal.infernitemod.client.InferniteClient"
    ],
    "main": [
      "net.flippycereal.infernitemod.Infernite"
    ],
    "fabric-datagen": [
      "net.flippycereal.infernitemod.InferniteDatagen"
    ],
    "rei_client": [
      "net.flippycereal.infernitemod.compat.InferniteREIPlugin"
    ]
  },
  "mixins": [
    "infernitemod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
